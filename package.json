  "bugs": {
    "url": "https://github.com/new-inventor/vue-pluralization/issues"
  },
  "homepage": "https://github.com/new-inventor/vue-pluralization#readme",
  "dependencies": {
    "@new-inventor/pluralization": "^1.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@new-inventor/packages-helper": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "*"
  ]
