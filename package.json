{
  "name": "@new-inventor/vue-pluralization",
  "version": "1.0.0",
  "description": "Vue pluralisation plugin for russian and english words can be extended by another languages",
  "keywords": [
    "pluralization",
    "pluralisation",
    "plural",
    "pluralize",
    "pluralise",
    "ending",
    "плюрализация",
    "числительные",
    "окончания",
    "word",
    "words",
    "слово",
    "vue",
    "plugin"
  ],
  "main": "index.js",
  "scripts": {
    "setup": "yarn install",
    "p:build": "tsc",
    "p:clear": "clear",
    "p:pack": "npm run eslint:lint && npm run p:clear && npm run p:build && npm run p:copy-files && pack",
    "p:copy-files": "copy_files",
    "p:release": "npm run eslint:lint && npm run p:clear && npm run p:build && npm run p:copy-files && npm publish lib --access public",
    "p:version": "version",
    "eslint:lint": "eslint ./src/**",
    "eslint:fix": "eslint --fix ./src/**",
    "eslint:dump": "eslint --print-config ./.eslintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/new-inventor/vue-pluralization"
